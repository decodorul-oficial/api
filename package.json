{
  "name": "monitorul-oficial-api",
  "version": "1.0.0",
  "type": "module",
  "description": "API GraphQL pentru Monitorul Oficial cu Supabase È™i Apollo Server",
  "main": "api/src/index.js",
  "scripts": {
    "start": "node api/src/index.js",
    "dev": "nodemon api/src/index.js",
    "build": "echo 'Build completed - no compilation needed for Node.js'",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint api/src/",
    "lint:fix": "eslint api/src/ --fix",
    "example:rate-limiting": "node examples/rate-limiting-example.js",
    "example:password-decryption": "node examples/password-decryption-example.js",
    "vercel-build": "npm run build",
    "vercel-dev": "npm run dev",
    "verify-deployment": "node scripts/verify-deployment.js"
  },
  "keywords": [
    "graphql",
    "apollo",
    "supabase",
    "api",
    "monitorul-oficial"
  ],
  "author": "Monitorul Oficial Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-depth-limit": "^1.1.0",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
